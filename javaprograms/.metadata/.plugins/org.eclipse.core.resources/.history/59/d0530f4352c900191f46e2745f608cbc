package task;

public class RepeatedNumber {

	public static void RepeatFinder(int a[])
	{
		int b[]=new int[2];
		int n=a.length;
		for(int i=0;i<n;i++)
		{
			for (int j=1;j<n;j++)
			{
				if(a[i]==a[j])
				{
					System.out.println("Repeated numbers found at locations "+i+" and "+j);
					
					break;
				}
			}
		}
	}
	
	public static void main(String[] args) {
		// TODO Auto-generated method stub

	}

}
