package task;

import com.bridgelabz.utility.Utility;

public class PrimeNumbersUpto1000 {

	public static void main(String[] args) {
	int a[][]=new int[10][];
	int x=0,y=0;
		for(int i=0;i<10;i++)
		{
			for(int j=0;j<100;j++)
			{
				if(Utility.isPrime(j)==false) a[i][j]=j;
			}
			
		}
		System.out.print(a[0][0]);
	}

}
