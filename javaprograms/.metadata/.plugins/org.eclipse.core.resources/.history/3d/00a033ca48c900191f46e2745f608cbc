package task;

import org.w3c.dom.css.Counter;

import com.bridgelabz.utility.Utility;

public class FrequencyCount {
	
	
	
	public static void FQ(String a[])
	{
		Utility.WordSort(a);
		int x=0,j=0;
		for(int i=1;i<a.length;i++)
		{
			if(!a[i].equals(a[i-1]))
			{
				x++;
			}
		}
		int b[]=new int[x];
		for(int i=1;i<a.length;i++)
		{
			
			if(a[i].equals(a[i-1]))
			{
				b[j]++;
			}
			else j++;
		}
	}

	public static void main(String[] args) {
		int x=0;

}
}
