package task;

public class RepeatedNumber {

	public static void RepeatFinder(int a[])
	{
		int b[]=new int[2];
		int n=a.length;
		for(int i=0;i<n;i++)
		{
			for (int j=1;j<n;j++)
			{
				if(a[i]==a[j])
				{
					System.out.println("Repeated numbers are found at locations "+i+" and "+j);
					b[0]=a[i];b[1]=a[j];
					for(int k=0;k<2;k++) System.out.println(b[k]+" ");
					break;
				}
			}
		}
	}
	
	public static void main(String[] args) {
		// TODO Auto-generated method stub
		int a[]=new int[100];
		for (int i=0;i<100;i++)
		{
			a[i]=i+1;
		}
		a[50]=2;
	}

}
