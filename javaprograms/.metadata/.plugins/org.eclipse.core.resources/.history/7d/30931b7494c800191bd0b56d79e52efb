package task;

public class PrimeNumbersUpto1000 {

	public static void main(String[] args) {
	int a[][]=new int[10][];
		int x=0,y=-1,n=1,p=0;
		
		for(int i=n;i<=n+100;i++)
		{
			if(i==0 || i==1) {x=1;break;}
			y++;
			for(int j=2;j<i/2;j++)
			{
				if(i%j==0 && i!=j) x=1;
			}
			
			if(x==0 || i==2) a[p][y]=i;
			
		}
		int len=a[0].length;
		for(int i=0;i<len;i++)
		{
			System.out.print(a[i]+" ");
		}

	}

}
