package task;

public class TwoDArray {

	public static void main(String[] args) {
		int a[][]=new int[3][4];
		for(int i=1;i<=4;i++)
		{
			for(int j=2;j<6;j++)
			{
				a[i][j]=i*j;
			}
		}
		for(int i=1;i<4;i++)
		{
			for(int j=2;j<6;j++)
			{
				System.out.print(a[i][j]+" ");
			}
			System.out.println();
		}
	}

}
