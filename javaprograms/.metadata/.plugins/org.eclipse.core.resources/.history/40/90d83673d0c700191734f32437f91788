package task;

public class InsertionSort {
	
	public static void Insertion(int a[])
	{
		int n=a.length;
		int temp=0;
		
		for(int i=0;i<n;i++)
		{
			temp=a[i];
			for(int j=i;j>0;j--)
			{
				if(a[j-1]>a[j])
				{
					a[j]=a[j-1];
				}
				else
				{
					a[j-1]=temp;
					break;
				}
			}
		}
		for(int i=0;i<n;i++)
		{
			System.out.print(a[i]+" ");
		}
	}

	public static void main(String[] args) {
		int a[]= {2,5,4,6,1,10,9};
		Insertion(a);
	}

}
