package JavaPrograms;

import java.util.Scanner;



public class Fibboncci {
	public static void main(String[] args) {
		int n;
		int n1=0;
		int n2=1;
		int n3=0;
		
		Scanner scanner=new Scanner (System.in);
		System.out.println("Enter the Number");
		n=scanner.nextInt();
	    
		for(int i=0;i<n;++i)
		{
			n3=n1+n2;
			System.out.println(n1+" "+n3);
			n1=n2;
			n2=n3;
		
		}
		
		
		}

}
